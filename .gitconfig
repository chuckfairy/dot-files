    email = charlieabeling@gmail.com
    name = chuckfairy

[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true

[push]
    default = current

[core]
    editor = vim

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[remote "origin"]
	fetch = +refs/pull/*/head:refs/remotes/origin/pr/*
